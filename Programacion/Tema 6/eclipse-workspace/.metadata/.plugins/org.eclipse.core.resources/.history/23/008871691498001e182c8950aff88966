package circulo;

public class Cilindro extends Circulo{
	private double height;
	
	public Cilindro(){
		this.height=1.0;
		this.radius=1.0;
	}
	
	public Cilindro(double newHeig){
		this.height=newHeig;
	}
	
	public Cilindro(double radio,double newHeig){
		this.radius=radio;
		this.height=newHeig;
	}
	
	public double getHeight() {
		return this.height;
	}
	
	public double getVolumen() {
		double area=this.getArea();
		return area*this.height;
	}
}
